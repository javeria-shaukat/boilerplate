{
    "name": "laravel/boilerplate-package",
    "description": "This is a simple boilerplate package of laravel",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "javeriaShuakt",
            "email": "116783325+javeriaShuakt@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2",
      
        "composer-plugin-api": "^2.0",
        "composer/composer": "^2.0", 
        "livewire/livewire": "^3.5",
        "power-components/livewire-powergrid": "^5.10",
        "laravel/breeze": "^2.1",
        "spatie/laravel-permission": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BoilerplatePackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\BoilerplatePackage\\BoilerPlateServiceProvider"
            ]
        },
        "class": "Laravel\\BoilerplatePackage\\InstallerPlugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
